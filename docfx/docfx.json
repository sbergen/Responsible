{
    "metadata": [
      {
        "src": [
          {
            "files": [ "Responsible.csproj" ],
            "src": "../"
          }
        ],
        "dest": "obj/api",
        "filter": "filter.yml"
      }
    ],
    "build": {
      "content": [
        {
          "files": [ "**/*.yml" ],
          "src": "obj/api",
          "dest": "api"
        },
        {
          "files": [ "*.md", "toc.yml" ]
        }
      ],
      "resource": [
        {
          "files": [ ]
        }
      ],
      "globalMetadata": {
        "_appTitle": "Responsible",
        "_enableSearch": false,
        "_disableContribution": true
      },
      "markdownEngineName": "markdig",
      "dest": "docs_site",
      "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
      "template": [ "statictoc" ]
    }
}
